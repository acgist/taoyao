<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- 是否开启预览 -->
    <bool name="preview">true</bool>
    <!-- 信令版本 -->
    <string name="version">1.0.0</string>
    <!-- 超时时间 -->
    <integer name="timeout">5000</integer>
    <!-- 终端类型：CAMERA|MOBILE -->
    <string name="clientType">MOBILE</string>
    <!-- 信令加密策略：AES|DES -->
    <string name="encrypt">DES</string>
    <!-- 信令加密密钥 -->
    <string name="encryptSecret">2SPWy+TF1zM=</string>
    <!-- 文件存储目录 -->
    <string name="storagePathImage">/taoyao/image</string>
    <string name="storagePathVideo">/taoyao/video</string>
    <!-- 传输类型：RTP|WEBRTC -->
    <string name="transportType">WEBRTC</string>
    <string name="audioRecordFormat"></string>
    <string name="videoRecordFormat"></string>
</resources>